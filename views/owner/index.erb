<h3>Here is the list of owners and their adopted cats</h3>
<a class="links" href="/owners/new">Create New Owner</a>
<% @owners.each do |owner| %>
<hr/>
   <p>Owner: <%= owner.name %></p>
   <ul>
     <% for cat in owner.cats %>
     <li><%= cat.name %></li>
     <% end %>
   </ul>
<p><a class="links" href="/owners/<%= owner.id %>">Show Owner</a></p>
<% end %>
